   "source": [
    "### EXERCISE 17 - Matrix Transpose\n",
    "\n",
    "Output the transpose of a matrix (as numpy array)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "523ae356",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "028c6620",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[1 2 3]\n",
      " [9 8 7]]\n"
     ]
    }
   ],
   "source": [
    "# 2x3 matrix\n",
    "\n",
    "a = np.array([[1,2,3], [9,8,7]])\n",
    "print(a)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "7fae8c05",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[1 9]\n",
      " [2 8]\n",
      " [3 7]]\n"
     ]
    }
   ],
   "source": [
    "# b = a transposed: 2x3 -> 3x2\n",
    "\n",
    "b = np.transpose(a)\n",
    "print(b)"
   ]
  }
